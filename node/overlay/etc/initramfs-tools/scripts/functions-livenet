
do_log() {
    [ -f /dev/ttyS0 ] && echo "$@" > /dev/ttyS0
    logger -n 192.168.124.1 -P 5140 -t "Livenet" "$@"
    #echo "$@" 
    if [ -x /bin/plymouth ] && plymouth --ping; then
        /bin/plymouth message --text="$@"
    fi
}

do_debug_shell() {
    loadkeys /etc/boottime.kmap.gz
    echo "Debugging shell"
    /bin/sh
}

